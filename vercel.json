{
  "version": 2,
  "buildCommand": "npm run build -- --configuration=production",
  "outputDirectory": "dist/farmaciasdeoccidente",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  }
}
